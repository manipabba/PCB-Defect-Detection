 not exist!\")\n","    count += 1\n","\n","  # write out files\n","  split_file.write(paths)\n","  split_file.close()\n","\n","\n","\n","\n","\n","# write content to directories\n","train, val = train_test_split(list(data_map.keys()), test_size=0.2)\n","write_data(\"train\", train)\n","write_data(\"val\", val)\n","  \n","\n","\n","  # img_attrs = anno.loc[anno['file'] == filename]\n","\n","  # for index, attr in img_attrs.iterrows():\n","  #    cv2.rectangle(img, (attr['xmin'], attr['ymin']), (attr['xmax'], attr['ymax']), color=(0,0,255), thickness = 10)\n","  #    cv2.putText(img, attr['class'], (attr['xmin'], attr['ymin']-10), cv2.FONT_HERSHEY_SIMPLEX, 0.9, (36,255,12), 2)\n","     "]},{"cell_type":"code","execution_count":null,"metadata":{"id":"hlFGgy9VH3AG"},"outputs":[],"source":["# # populate directories with actual data\n","\n","# from sklearn.model_selection import train_test_split\n","# import shutil\n","\n","\n","# # split into train and val data\n","# train, val = train_test_split(anno, test_size=0.2)\n","\n","# def write_data(split_type, df):\n","#   split_file = open(f\"/content/datasets/pcb/{split_type}.txt\", \"w\")\n","#   paths = \"\"\n","#   for index, row in df.iterrows():\n","#     # test this by maybe plotting out some of these\n","#     base = f\"im{index}\"\n","#     i_w, i_h = row[\"width\"], row[\"height\"]\n","#     x_min, x_max = row[\"xmin\"] / i_w, row[\"xmax\"] / i_w\n","#     y_min, y_max = row[\"ymin\"] / i_h, row[\"ymax\"] / i_h\n","#     w, h = x_max - x_min, y_max - y_min\n","\n","#     # write label\n","#     label_file = open(f\"/content/datasets/pcb/labels/{base}.txt\", \"w\")\n","#     label_file.write(f\"0 {(x_max - x_min) / 2} {(y_max - y_min) / 2} {w} {h}\")\n","#     label_file.close()\n","\n","#     # copy over respective img file\n","#     try:\n","#       shutil.copyfile(row[\"file\"], f\"/content/datasets/pcb/images/{base}.jpg\")\n","#       paths += f\"/content/datasets/pcb/images/{base}.jpg\" + \"\\n\"\n","#     excep